{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "outDir": "../",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "ts/**/*.ts",
        "ts/**/*.tsx",
        "!node_modules/**"
    ],
    "files": [
        "ts/action/Action.ts",
        "ts/action/ActionDispatcher.ts",
        "ts/action/ActionListener.ts",
        "ts/action/impl/LoginAction.ts",
        "ts/action/impl/LogoutAction.ts",
        "ts/action/impl/PageRequestAction.ts",
        "ts/action/impl/RefreshCirclesAction.ts",
        "ts/action/impl/RefreshFriendRequestsAction.ts",
        "ts/action/impl/RefreshFriendsAction.ts",
        "ts/action/impl/SearchResultAction.ts",
        "ts/auth/ILoginState.ts",
        "ts/auth/IRegisterState.ts",
        "ts/content/IContentPanelState.ts",
        "ts/content/page/IPageProps.ts",
        "ts/content/page/IRequestsState.ts",
        "ts/content/search/ISearchContainerState.ts",
        "ts/content/search/user/IUserBoxProps.ts",
        "ts/content/search/user/IUserBoxState.ts",
        "ts/content/search/user/option/IUserBoxOptionsProps.ts",
        "ts/data/ApiRequest.ts",
        "ts/data/AppData.ts",
        "ts/data/model/Circle.ts",
        "ts/data/model/Friend.ts",
        "ts/data/model/FriendRequest.ts",
        "ts/data/model/Token.ts",
        "ts/data/model/User.ts",
        "ts/IAppProps.ts",
        "ts/IAppState.ts",
        "ts/nav/INavPanelState.ts",
        "ts/nav/profile/IProfileDataState.ts",
        "ts/App.tsx",
        "ts/auth/Login.tsx",
        "ts/auth/Register.tsx",
        "ts/content/ContentPanel.tsx",
        "ts/content/page/CreateCircle.tsx",
        "ts/content/page/Invites.tsx",
        "ts/content/page/Requests.tsx",
        "ts/content/search/SearchContainer.tsx",
        "ts/content/search/user/option/UserBoxOptions.tsx",
        "ts/content/search/user/UserBox.tsx",
        "ts/nav/NavPanel.tsx",
        "ts/nav/profile/ProfileData.tsx"
    ],
    "exclude": [],
    "atom": {
        "rewriteTsconfig": true
    },
    "compileOnSave": false,
    "buildOnSave": false
}
